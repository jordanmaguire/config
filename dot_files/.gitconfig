[user]
  name = Jordan Maguire
  email = a.jordan.maguire@gmail.com
[alias]
  br = branch
  co = checkout
  cp = cherry-pick
  df = diff --color
  fuckit = reset head --hard
  fi = reset head --hard
  rb = rebase --committer-date-is-author-date
  st = status --porcelain
[push]
  default = upstream
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.global_gitignore
  editor = subl -n -w
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
